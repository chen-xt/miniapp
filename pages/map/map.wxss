/* 弹框样式 */
.modal-dialog{
    width: 100%;
}
.modal-content{
  padding: 20rpx 35rpx;
  border: 1rpx solid  rgba(40, 40, 40, 0.22);
}
.chargeInfo{
    font-size: 28rpx;
    color: #555;
    line-height: 50rpx;
}
.chargeInfo text{
    color: #000;
}
.chargeInfo image{
    margin-right: 10rpx;
    width: 28rpx;
    height: 28rpx;
}
.modal-footer{
    margin: 10rpx 0 20rpx 0;
    text-align:center;

}
.modal-footer view{
    display: inline-block;
    width: 48%;
    text-align: center;
    color: #000;
    font-size: 30rpx;
}
.modal-footer image{
    width: 30rpx;
    height: 30rpx;
}


/*授权弹框*/
.modelOne{
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  width: 100%;
  height: 100%;
  z-index: 9999;
  background-color: rgba(51, 51, 51, 0.6);
  transition: all .15s linear;
}
.modelOne-title{
  position: relative;
  background-color: #fff;
  margin: 0 auto;
  width: 500rpx;
  top:42%;
  transform: translateY(-50%);
  border-radius: 5px;
  text-align: center;
}
.modelOne-title view{
  padding-top: 40rpx;
  border-bottom: 1px solid #e4e4e4;
  font-size: 36rpx;
}
.modelOne-title .texo{
    display: block;
    font-size: 32rpx;
    color: #909090;
    padding: 40rpx 0 ;
    border-bottom: 1rpx solid #d2d2d2;
}
.modelOne-title button{
    width: 100%;
    background-color:transparent !important;
    color: #00D44B !important;
}
.modelOne-title button::after{ 
    border: none; 
}
.modelOne-title .title{
    padding: 20rpx;
    border-bottom: 1rpx solid #d2d2d2;
}
.monthCard{
    position: absolute;
    left: 20rpx;
    bottom: 260rpx;
    height: 108rpx;
    width: 108rpx;
}
  